<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.corfoarte.victorcastro.corpoarte">

    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:allowBackup="true"
        android:icon="@drawable/gt"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LoginActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.corfoarte.victorcastro.corpoarte.LoginActivity" />
        </activity>
        <activity
            android:name=".Grupos.GruposActivity"
            android:label="@string/title_activity_grupo"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Grupos.RegistrarGrupoActivity" />
        <activity android:name=".Grupos.ListarGruposActivity" />
        <activity android:name=".InscripcionesActivity" />
        <activity
            android:name=".NoticiasActivity"
            android:label="@string/title_activity_noticias" />
        <activity
            android:name=".SplashScreenActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HomeActivity"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
    </application>

</manifest>